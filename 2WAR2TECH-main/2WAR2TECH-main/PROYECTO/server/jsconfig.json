{
    "exclude": ["node_modules"],
    "compilerOptions": {
        "module": "Node16",
        "baseUrl": "./",
        "paths": {
            "#Config/*": ["./src/config/*"],
            "#Controllers/*": ["src/controllers/*"],
            "#Dto/*": ["./src/dto/*"],
            "#Routes/*": ["./src/routes/*"],
            "#Models/*": ["./src/models/*"],
            "#Helpers/*": ["./src/helpers/*"],
            "#Middleware/*": ["./src/middleware/*"],
        }
    }
}